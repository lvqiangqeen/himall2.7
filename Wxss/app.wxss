 /*加入购物车弹出框*/
 

.product-buy{
  overflow: hidden;
  border:1px solid #ff8a65;
  border-radius: 20px;
  text-align: center;
  width:256rpx;
  height: 72rpx;
  line-height: 72rpx;
}
.product-buy .l,.product-buy .r{
  width: 70rpx;
  float: left;
  color: #ff8a65;
}
.product-buy .m{
   width:112rpx; text-align: center;
   line-height: 70rpx;
   font-size: 36rpx;
   float: left;
   color:black;
}

.product-buy input.m {
   font-size: 36rpx;
}
.product-buy .r{
  float: right;
}

.mask {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10;
  bottom:0;
  right:0;
  background-color: rgba(0, 0, 0, 0.4);
}

.comm-stand{
 margin: 55rpx;
   background: #fff; 
   border-radius: 12rpx;  
   box-shadow: 0rpx 0rpx 15rpx #ccc;
   position: fixed;
   top:15%;
   left: 0rpx;
   right: 0rpx;
}

.comm-stand-con{
    padding:32rpx 32rpx 20rpx 32rpx;
    position: relative;
    overflow: hidden;
}

.comm-stand .comm-item{ overflow: hidden;
   margin-bottom: 12rpx; line-height: 40rpx; clear: both;
}
.comm-attr{ overflow: hidden;}
.comm-stand .comm-name{
   font-size: 32rpx; 
   line-height: 40rpx;
   padding-bottom: 24rpx;
    margin-bottom: 24rpx;
   border-bottom: 1rpx solid #e0e0e0;   
}

.comm-stand .comm-name .comm-txt{
   display:block;white-space:nowrap; overflow:hidden; text-overflow:ellipsis;
     width:453rpx;
}
.comm-stand .comm-icon{
   position: absolute;
   width: 40rpx;
   height: 40rpx;
   top: 24rpx;
   right: 28rpx;
}

.comm-stand .comm-icon image{
   width: 40rpx;
   height: 40rpx;
}

.comm-item .comm-title{
   font-size: 28rpx;
   line-height: 44rpx;
   margin-bottom:12rpx;
}

.comm-item .comm-attr text{
     border: 1rpx solid #fff;
  padding:0 24rpx; line-height: 56rpx;
  display: block;
  float: left;
  margin-top: 12rpx;
  margin-bottom:12rpx;
  margin-right: 24rpx; white-space: nowrap;
  border-radius: 5px;
  background: #f5f5f5;
  font-size: 24rpx;
}

.comm-item .comm-attr text.active{
  border: 1rpx solid #ff5722;
  background: #ff5722;color: #fff;
}

.comm-stand-shop{
  background: #f5f5f5;
  overflow: hidden;
  border-bottom-left-radius: 12rpx;
  border-bottom-right-radius: 12rpx;
}

.comm-stand-shop .product-into {
   margin:24rpx 32rpx;
   height:72rpx;
line-height:72rpx;
color:#ff8a65;
}

.comm-stand-shop .product-into .product-price {
   color: #ff5722;
   font-weight: bold;font-size: 36rpx;
   width:200rpx;
   float:left;
}

.product-shop{
  float: right;
}

.product-shop .icon__plus{font-size:72rpx;}



/*地址信息区域*/
.address-info{
  border-top:1rpx solid #e0e0e0;
}
.address-info .address-outer{
  display:flex;
  padding:24rpx;
  padding-left:96rpx;
  background:#fff;
  overflow:hidden;
  position:relative;

}
.address-info .icon__locate{
  margin-right:24rpx;
  height:38rpx;
  position:absolute;
  left:24rpx;
  top:50%;
  margin-top:-24rpx;

}
.address-info .infos {
  display: flex;
  width: 600rpx;
  flex-direction: column;
}
.address-info .address {
  font-size: 28rpx;
  line-height: 40rpx;
  color: #616161;
}
.address-info .name {
  margin-bottom: 6rpx;
  font-size: 32rpx;
  line-height: 40rpx;
}
.address-info .addr-xuanbg {
  background: #fff;
  padding: 0;
  height: 11rpx;
  overflow: hidden;
}
.address-info .addr-xuanbg image {
  width: 750rpx;
  height: 11rpx;
  display: block;
}
.address-info .arrow_right {
  position:absolute;
  height: 20rpx;
  top: 50%;
  margin-top: -12rpx;
  right:24rpx;
  font-size:28rpx;
  color:#9e9e9e;
}
