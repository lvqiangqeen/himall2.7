/*加入购物车标志*/
.shop_box{position:fixed;
bottom:310rpx;
right:24rpx;
width:96rpx;
height:96rpx;
z-index:99;
background:#979797;
border-radius:50%;
border:1rpx solid #7c7c7c;
line-height:96rpx;
text-align:center;
}
.shop_box .shop_icon{ position: relative;color:#fff;}
.shop_num{ position:absolute; border:1px #ff5757 solid; top:12rpx;right:10rpx;background:#f44336;color:#fff;width:29rpx;height:29rpx;line-height:29rpx;border-radius:100%;font-size:20rpx;text-align:center;
animation-name: shop_num;
      animation: shop_num 2s alternate infinite;
      -webkit-animation: shop_num 2s alternate infinite;
      -moz-animation: shop_num 2s alternate infinite;
} 
 
  @keyframes shop_num{
    0%{ opacity: 1;}
    50%{ opacity: 1}
    100%{ opacity: 1}
 } 
