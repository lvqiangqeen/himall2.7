  url('https://at.alicdn.com/t/font_5w1wc566q5kd42t9.ttf') format('truetype'),
  url('https://at.alicdn.com/t/font_5w1wc566q5kd42t9.svg#iconfont') format('svg');
}

 
.iconfont {
  font-family: "iconfont" !important;
  font-size: 48rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

