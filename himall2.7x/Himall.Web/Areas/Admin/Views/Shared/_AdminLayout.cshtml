<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit|ie-stand">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/custom.css" rel="stylesheet" />
    <link href="~/Content/artdialog.css" rel="stylesheet" />
    <link href="~/Content/inner-custom.css" rel="stylesheet" />
    <style>
        .aside, .content {
            top: 51px;
        }

        .block {
            width: 64px;
            height: 1px;
            background: #e3393c;
            position: absolute;
            bottom: 1px;
            z-index: 9999;
            transition: left 0.3s ease;
        }

        .navbar-right li:before {
            content: "|";
            color: #565556;
            font-size: 12px;
            position: relative;
            top: 17px;
            float: left;
        }
    </style>
    @RenderSection("Styles", false)
    <script src="~/Scripts/jquery-1.11.1.min.js"></script>
    <script src="~/Scripts/jquery.form.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.artDialog.js"></script>
    <script src="~/Scripts/artDialog.iframeTools.js"></script>

    <!--[if lt IE 9]>
    <script src="/Scripts/respond.min.js"></script>
    <![endif]-->
    <script src="~/Scripts/jquery.nicescroll.min.js"></script>
    <script src="~/Scripts/inner-custom.js"></script>
    <script src="~/Scripts/CommonJS.js"></script>
    <title>木尚庭家居管理中心</title>
	@RenderSection("HeadScript", false)
</head>
<body>
    
    @{Html.RenderAction("Top", "AdminPublic");}
    @RenderBody()
    @RenderSection("Scripts", false)
    @{Html.RenderAction("Bottom", "AdminPublic");}
	@Html.Partial("_SelectMenuItem")
</body>
</html>
